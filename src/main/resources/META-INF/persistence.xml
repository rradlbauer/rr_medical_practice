<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

    <persistence-unit name="med">
        <class>at.spengergasse.medical_practice.entities.Adress</class>
        <class>at.spengergasse.medical_practice.entities.Anamnesis</class>
        <class>at.spengergasse.medical_practice.entities.Insurance</class>
        <class>at.spengergasse.medical_practice.entities.Patient</class>
        <class>at.spengergasse.medical_practice.entities.Schedule</class>
        <class>at.spengergasse.medical_practice.entities.Symptom</class>
        <class>at.spengergasse.medical_practice.entities.SymptomCode</class>
        <class>at.spengergasse.medical_practice.entities.Visit</class>
        <class>at.spengergasse.medical_practice.entities.Diagnosis</class>
        <class>at.spengergasse.medical_practice.entities.DiagnosisCode</class>
        <class>at.spengergasse.medical_practice.entities.Treatment</class>
        <class>at.spengergasse.medical_practice.entities.Medication</class>
        <class>at.spengergasse.medical_practice.entities.PhysicalTherapy</class>
        <class>at.spengergasse.medical_practice.entities.Agent</class>

        <properties>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver" />
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost/med_practice" />
            <property name="javax.persistence.jdbc.password" value=""/>
            <property name="javax.persistence.jdbc.user" value="root"/>
            <property name="javax.persistence.schema-generation.database.action" value="none" />
            <property name="hibernate.show_sql" value="true" />
        </properties>
    </persistence-unit>
</persistence>